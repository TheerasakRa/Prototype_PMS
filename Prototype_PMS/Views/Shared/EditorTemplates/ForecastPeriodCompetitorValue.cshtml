@model Prototype_PMS.Models.ForecastPeriodCompetitorValue

@Html.HiddenFor(m => m.ID)
@Html.HiddenFor(m => m.IsDelete)
@Html.HiddenFor(m => m.IsLastDelete)
@Html.HiddenFor(m => m.CreateDate)
@Html.HiddenFor(m => m.UpdateDate)
@Html.HiddenFor(m => m.UpdateBy)
@Html.HiddenFor(m => m.CreateBy)
@Html.HiddenFor(m => m.ForecastPeriodID)


@if (Model.IsDelete != true)
{
    <tr>
        <td>
            @Html.EditorFor(m => m.Detail, new { HtmlAttributes = new { @class = "form-control" } })
        </td>
        <td>
            <input value="ลบ" type="button" class="btn btn-outline-warning" onclick="FuncDelCompetitorValue(@Html.IdFor(m => m.IsDelete))" />
        </td>
    </tr>
}