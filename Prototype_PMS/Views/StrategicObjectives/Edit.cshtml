@model Prototype_PMS.Models.StrategicObjective

@{
    ViewBag.Title = "Edit";
}

<h4>จัดการวัตุประสงค์เชิงยุทธศาสตร์ตามแผนวิสาหกิจ</h4>


@using (Html.BeginForm("Edit", "StrategicObjectives", FormMethod.Post, new { role = "form", id = "myform", name = "myform" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => m.isDelete)
    @Html.HiddenFor(m => m.isLastDelete)
    @Html.HiddenFor(m => m.CreateDate)
    @Html.HiddenFor(m => m.UpdateDate)
    @Html.HiddenFor(m => m.No)
    @Html.HiddenFor(m => m.ID)
    @Html.HiddenFor(m => m.SOEPlanID)

    <div class="row mb-3 my-3">
        <label class="col-sm-2 col-form-label">รอบการแก้ไข</label>
        <div class="col-md-5">
            <div class="row">
                <div class="col">
                    <p>@Html.EditorFor(model => model.No, new { htmlAttributes = new { @class = "form-control" } }) </p>
                </div>
            </div>
        </div>
    </div>
    <div class="row mb-3 my-2">
        <label class="col-sm-2 col-form-label">วัตถุประสงค์เชิงยุทธศาสตร์</label>
        <div class="col-md-5">
            <div class="row">
                <div class="col">
                    <p>@Html.EditorFor(model => model.StrategicObjective1, new { htmlAttributes = new { @class = "form-control" } }) </p>
                </div>
            </div>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Save" class="btn btn-default" />
        </div>
    </div>

}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
